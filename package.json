    "jspm-github": "^0.12.2",
    "jspm-npm": "^0.23.2",
    "jspm-registry": "^0.4.0",
    "liftoff": "^2.1.0",
    "minimatch": "^2.0.8",
    "mkdirp": "~0.5.1",
    "ncp": "^2.0.0",
    "request": "^2.58.0",
    "rimraf": "^2.4.0",
    "rsvp": "^3.0.18",
    "semver": "^5.0.1",
    "systemjs": "^0.18.2",
