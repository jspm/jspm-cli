{
  "defaultRegistry": "jspm",
  "strictSSL": true,
  "registries": {
    "github": {
      "handler": "jspm-github",
      "remote": "https://github.jspm.io"
    },
    "npm": {
      "handler": "jspm-npm",
      "remote": "https://npm.jspm.io"
    },
    "jspm": {
      "handler": "jspm-registry",
      "remote": "https://registry.jspm.io"
    }
  }
}
